---
import LinkButton from "./LinkButton.astro";
const { links } = Astro.props;
---

<div class="link-buttons">
  <iframe
    style="border-radius:12px"
    src="https://open.spotify.com/embed/playlist/43mnKzy2LaOV1q6LhsJERY?utm_source=generator"
    width="100%"
    height="152"
    frameborder="0"
    allowfullscreen=""
    allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"
    loading="lazy"></iframe>
  {links.map((link: { url: any; title: any; }) => <LinkButton href={link.url} text={link.title} />)}
</div>
